<div>
  <%= form_for [@route, @attempt] do |f| %>
    <%= f.label :athlete %>
    <%= f.select :name %>

    <%= f.label :scored_holds, "Scored Holds" %>
    <%= f.number_field :scored_holds %>

    <%= f.submit (f.object.new_record? ? "Create Round" :
      "Update Round"), class: "button tiny success" %>
  <% end %>
</div>
