<h2><%= @competition.name %> - <%= @round.name %> (Round <%= @round.number %>)
</h2>
<%= @competition.gender %>  <%= @competition.division %><br>

<h3><%= @route.name %></h3>

<br>
<% if current_user == @competition.user %>
  <%= link_to "Record Attempt", new_route_attempt_path(@route), class: "button tiny" %>
  <%= link_to "Edit Route", edit_route_path(@route), class: "button tiny" %>
<% end %>



<%= link_to "All Attempts", route_attempts_path(@route) %>
